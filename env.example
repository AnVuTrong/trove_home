# Environment configuration for Trove Home Docker builds
# Copy this file to `.env` and replace the placeholder values.

# Docker Hub username (required for build_images.sh)
DOCKER_USER=<your-docker-hub-username>
DOCKER_HUB_ACCESS_TOKEN=<your-docker-hub-access-token>
MAJOR_VERSION=0
MINOR_VERSION=dev 

# VPS SERVER:
VPS_IP=
VPS_USERNAME=
VPS_PASSWORD=
VPS_SSH_PORT=